;\nimport { Component, Vue, Prop } from \"vue-property-decorator\";\nimport focus from \"@/directives/focus-directive\";\nimport shows from \"@/shows\";\n\ninterface Show {\n  id: string;\n  title: string;\n  description: string;\n  genres: string[];\n  urlImage: string;\n  status: \"Ended\" | \"Continuing\";\n  dateCreation: string;\n  numberOfSeasons: string;\n  isFavorite: boolean;\n}\n\n@Component({\n  components: {\n    card: Card\n  },\n  directives: {\n    focus\n  }\n})\nexport default class HomeComponent extends Vue {\n  title = \"DÃ©tail show\";\n  searchQuery = \"\";\n  validatedSearchQuery = \"\";\n  shows: ReadonlyArray<Show> = shows;\n\n  @Prop({ required: true })\n  id!: string;\n\n  validateSearchQuery(): void {\n    this.validatedSearchQuery = this.searchQuery;\n  }\n\n  get show(): Show | undefined {\n    return this.shows.find(show => show.id === this.id);\n  }\n}\n</script>\n\n<style>\n.navbar-item.is-active {\n  color: #00d1b2 !important;\n}\n\n.card-result {\n  margin-bottom: 20px;\n}\n\n.is-128x200 {\n  height: 200px;\n  width: 128px;\n}\n\n.is-favorite {\n  color: #ffdd57;\n}\n</style>\n"]}]}